{
    "name": ":vendor_name/:package_name",
    "description": ":package_description",
    "type": ":type",
    "license": ":license",
    "authors": [
        {
            "name": ":author_name",
            "email": ":author_email",
            "homepage": ":author_homepage"
        }
    ],
    "autoload": {
        "psr-4": {
            "Undeadline\\:autoload_package_name": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": ":stability",
    "prefer-stable": true,
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10"
    }
}
